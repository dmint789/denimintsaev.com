.table-headings,
.table-data,
.table-headings > th,
.table-data > td {
  @apply text-center border-mygray-500;
}

.table-headings > th {
  @apply h-10;
}

.table-headings > th:not(:last-child),
.table-data > td:not(:last-child) {
  @apply border-r-2;
}

.big-input-output-box {
  @apply w-full px-3 py-2 border-2 border-black focus:border-black outline-none bg-white hover:bg-white;
}
